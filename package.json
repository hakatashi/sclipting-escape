{
  "name": "sclipting-escape",
  "version": "1.1.0",
  "description": "A tiny node.js module to escape String/Number/Buffer into ByteArray literal of Sclipting esoteric programming language.",
  "main": "index.js",
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "prepublish": "tsd install",
    "test": "mochify test.coffee --node --reporter spec --extension .ts --extension .coffee --transform coffeeify --plugin [ tsify ]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hakatashi/sclipting-escape.git"
  },
  "keywords": [
    "converter",
    "escape",
    "sclipting",
    "esolang"
  ],
  "author": "Koki Takahashi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hakatashi/sclipting-escape/issues"
  },
  "homepage": "https://github.com/hakatashi/sclipting-escape#readme",
  "devDependencies": {
    "chai": "^3.4.1",
    "coffeeify": "^1.1.0",
    "core-js": "^1.2.6",
    "mocha": "^2.3.4",
    "mochify": "^2.14.1",
    "tsd": "^0.6.5",
    "tsify": "^0.12.2"
  }
}
